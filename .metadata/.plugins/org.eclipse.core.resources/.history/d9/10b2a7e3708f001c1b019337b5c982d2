package com.training;
import java.util.*;
public class DigitSum {

	public static void main(String[] args) {
//				674 % 10 = 4
//				Sum = 0 + 4 = 4
//				674 / 10 = 67
//				Second iteration
//				67 % 10 = 7
//				Sum = 4 + 7 = 11
//				67 / 10 = 6
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
//		int sum = 0;
		for(int i=0;i<n;i++) {
			int r = n%10;
			int sum = 0 + r;
			System.out.println(sum);
		}
	}

}
