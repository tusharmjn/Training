package com.training.oop;

class Shapes {
    protected String name;
    protected String color;

    Shapes(String n, String c) {
        name = n;
        color = c;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getColor() {
        return color;
    }

    public void setColor(String color) {
        this.color = color;
    }

}

class Circleone extends Shapes {

    Circleone(String n, String c) {
        super(n, c);

    }

}

class Rectangle extends Shapes {

    Rectangle(String n, String c) {
        super(n, c);

    }

}

class Square extends Shapes {

    Square(String n, String c) {
        super(n, c);
        // TODO Auto-generated constructor stub
    }

}

public class AreaShapes {

    public static void main(String[] args) {
        Shapes sh = new Shapes(String name, int length, int width, int radius);
        
       
        
        
    }

}
