package com.training;
import java.util.*;

class Circle {
    String name;
    float radius;
    String color;
}

public class faltu {
    public static void main(String[] args) {
        Circle c1 = new Circle();
        Scanner sc = new Scanner(System.in);
        float r = sc.nextFloat();
        c1.radius = r;
        System.out.println(c1.radius);
        
    }

}
