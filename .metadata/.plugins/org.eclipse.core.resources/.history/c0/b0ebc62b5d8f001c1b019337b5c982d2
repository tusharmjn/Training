package com.training;
import java.util.*;

public class ShiftZeros {

	public static void main(String[] args) {
		int []a = {1,9,8,4,0,0,2,7,0,6,0};
		
		ArrayList<Integer> Arr = new ArrayList<Integer>();
		Arr.add(a[0]);
		Arr.add(a[1]);
		Arr.add(a[2]);
		Arr.add(a[3]);
		Arr.add(a[4]);
		for(int i=0;i<Arr.size();i++) {
			System.out.print(Arr.get(i) + " ");
			
		}
	}

}
